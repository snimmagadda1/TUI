

# The primary network interface 
auto br0
iface br0 inet static
        address 10.0.2.6
        netmask 255.255.255.0
        gateway 10.0.2.1
        bridge_ports eth0
        bridge_maxwait 0
        bridge_stp on
        dns-nameservers 10.64.5.1
        dns-serach pulse-64-5-0.rlpulse.net rlsoulutions.com

